const prompt = require("prompt-sync")();
// While loop
// con :- if the satisfying condn never becomes false, the loop becomes infinite
let a = prompt("Enter any number -> ");
a = Number.parseInt(a);
let i = 0;
while (i <= a) {
  console.log(i);
  i++;
}

// Do-While loop :- the execution will compulsorily take place atleast once since it check the condn at the exit point
let b = prompt("Enter a number -> ");
b = Number.parseInt(b);
let j = 5;
do {
  console.log(j);
  j++;
} while (j < b);
